{
    "manifest_version": 1,
    "version": 1,
    "type": "APP",
    "name": {
        "ja": "文字結合プラグイン",
        "en": "Text Connect",
        "zh": "字符结合插件"
    },
    "description": {
        "ja": "複数のフィールドの文字を連結して、特定のフィールドに代入するプラグインです。",
        "en": "This plug-in can combine the text of one field with another field.",
        "zh": "此插件用于结合多个字段的文字，并显示到特定的字段中."
    },
    "icon": "contents/icon.png",
    "desktop": {
        "js": [
            "https://js.cybozu.cn/jquery/1.11.1/jquery.min.js",
            "desktops/desktop.js"
        ]
    },
    "config":{
        "html": "contents/setting.html",
        "js": [
            "https://js.cybozu.cn/jquery/1.11.1/jquery.min.js",
            "https://js.cybozu.cn/sweetalert/v1.1.3/sweetalert.min.js",
            "config_js/config.js"

        ],
        "css": [
                "config_css/config.css",
                "https://js.cybozu.cn/sweetalert/v1.1.3/sweetalert.css"
        ],
        "required_params": ["copyfield"]
    }
}
